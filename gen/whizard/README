  e2e2_eRpL.sin  // RHN --> mu + W, right beam pol 
  e3e3_eLpR.sin  // RHN --> tau + W, left beam pol
  e3e3_eRpL.sin  // RHN --> tau + W, right beam pol 

  common parameters:
	ECM = 250GeV
	M(RHN) = 20 GeV
	W(RHN) = 0.01 GeV
	M(Zp) = 7500 GeV
	g1p = 0.75

